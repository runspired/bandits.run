{
  "version": 1,
  "description": "Cloudflare Pages routing configuration for client-side SPA routing",
  "include": ["/*"],
  "exclude": [
    "/api/*",
    "/fonts/*",
    "/images/*",
    "/map-styles/*",
    "/*.json",
    "/*.ico",
