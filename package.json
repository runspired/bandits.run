{
  "name": "bandits-web",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for bandits-web goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#api/GET": "./app/data/api/get.ts",
    "#config": "./app/config/environment",
    "#components/*": "./app/components/*",
    "#entities/*": "./app/components/entities/*",
    "#ui/*": "./app/components/ui/*",
    "#ux/*": "./app/components/ux/*",
    "#layout/*": "./app/components/layout/*",
    "#maps/*": "./app/components/maps/*",
    "#core/*": "./app/components/core/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite",
    "test": "vite build --mode development && testem ci --port 0",
    "strava:auth": "node scripts/strava-auth.ts",
    "strava:fetch-events": "node scripts/fetch-strava-events.ts",
    "strava:fetch-route": "node scripts/fetch-strava-route.ts",
    "api:compile": "node api/compile.ts",
    "api:typecheck": "tsc --project api/tsconfig.json"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/core": "^4.4.2",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.6",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.0",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.8",
    "@rollup/plugin-babel": "^6.1.0",
    "@shikijs/markdown-it": "^3.20.0",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.11",
    "@types/node": "^22.19.3",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "@warp-drive/core": "5.8.0",
    "@warp-drive/ember": "5.8.0",
    "@warp-drive/json-api": "5.8.0",
    "@warp-drive/legacy": "5.8.0",
    "@warp-drive/utilities": "5.8.0",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-cli-deprecation-workflow": "^4.0.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-primitives": "^0.48.2",
    "ember-qunit": "^9.0.4",
    "ember-scoped-css": "^2.0.4",
    "ember-source": "~6.10.0-beta.1",
    "ember-template-lint": "^7.9.3",
    "ember-strict-application-resolver": "^0.1.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "eslint-plugin-warp-drive": "5.8.0",
    "globals": "^16.5.0",
    "markdown-it": "^14.1.0",
    "leaflet": "^1.9.4",
    "@types/leaflet": "^1.9.21",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-container": "^4.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "shiki": "^3.20.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^38.0.0",
    "testem": "^3.17.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">= 24"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0"
  }
}
